local Rayfield = loadstring(game:HttpGet('https://sirius.menu/rayfield'))()
--Window
local Window = Rayfield:CreateWindow({
   Name = "Skibidi Hub",
   Icon = 0, 
   LoadingTitle = "Skibidi Hub",
   LoadingSubtitle = "Skibidi Hub Loading...",
   Theme = "DarkBlue", 

   DisableRayfieldPrompts = false,
   DisableBuildWarnings = false,

   ConfigurationSaving = {
      Enabled = true,
      FolderName = nil,
      FileName = "SKIBIDI HUB"
   },

   Discord = {
      Enabled = false,
      Invite = "noinvitelink", 
      RememberJoins = true 
   },

   KeySystem = true,
   KeySettings = {
      Title = "Skibidi Key System",
      Subtitle = "Key System",
      Note = "KEY IS IN DISCORD",
      FileName = "KEYFORSKIBIDIHUB",
      SaveKey = true,
      GrabKeyFromSite = false,
      Key = {"FF37YDSHU0V"}
   }
})
 
--Notification

Rayfield:Notify({
   Title = " Skibidi Notification",
   Content = "Thanks For Using Skibidi Hub!",
   Duration = 6.5,
   Image = 4483362458,
})



